    "eslint-plugin-react": "^7.20.0",
    "eslint-scope": "^5.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "redux-devtools-extension": "^2.13.8",
